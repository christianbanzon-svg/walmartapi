# Copy this file to .env and fill in your values

# --- Required ---
BLUECART_API_KEY=

# Domain to target (supported: walmart.com, walmart.ca)
WALMART_DOMAIN=walmart.com

# --- Optional API overrides ---
BLUECART_BASE_URL=https://api.bluecartapi.com/request

# --- Output & storage ---
# Inside container, compose maps ./walmart/output -> /data/output
OUTPUT_DIR=/data/output
DATABASE_PATH=/data/walmart.sqlite3

# --- Runtime controls (used by docker-compose command)
KEYWORDS=instyler
MAX_PER_KEYWORD=10
SLEEP=0.0
MAX_PAGES=50
RETRY_SELLER_PASSES=0
RETRY_SELLER_DELAY=15.0
ZIPCODE=
CATEGORY_ID=
